## Running the development server
## commands to set up demo
pip install -r requirements.txt
source PENV/bin/activate
python shortie/manage.py create_db
python shortie/manage.py pop_db
supervisord -c etc/supervisor/conf.d/shortie-dev.conf

## commands to run to use API
curl -X POST -H 'Content-Type: application/json' -d'{"urls": {"desktop":"https://google.com"}}' localhost:9001/api/v1/users/jason@seaver.com/urls
curl -X GET -H'User-Agent: Mozilla/5.0(iPad; U; CPU iPhone OS 3_2 like Mac OS X; en-us) AppleWebKit/531.21.10 (KHTML, like Gecko) Version/4.0.4 Mobile/7B314 Safari/531.21.10' localhost:9001/50617f9
curl -X GET localhost:5000/api/v1/users/jason@seaver.com/urls
